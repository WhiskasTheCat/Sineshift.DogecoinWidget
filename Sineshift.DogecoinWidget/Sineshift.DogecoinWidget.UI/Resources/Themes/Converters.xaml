<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Sineshift.DogecoinWidget.UI"
					 xmlns:cui="clr-namespace:Sineshift.DogecoinWidget.Common.UI;assembly=Sineshift.DogecoinWidget.Common.UI">


	<cui:BoolToValueConverter x:Key="Converter.BoolToVisibility"
							  TrueValue="{x:Static Visibility.Visible}"
							  FalseValue="{x:Static Visibility.Collapsed}" />

	<cui:BoolToValueConverter x:Key="Converter.BoolToVisibility.Reverse"
							  TrueValue="{x:Static Visibility.Collapsed}"
							  FalseValue="{x:Static Visibility.Visible}" />
</ResourceDictionary>